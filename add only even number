import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner scan = new Scanner(System.in);
	    int a = scan.nextInt();
	    System.out.println(a);
	    int sum=0;
	    while(a>0){
	        {
	        int dig=a%10;
	        if(dig%2==0){
	        sum=sum+dig;
	        }
	        a=a/10;
	        
	        }
	    }
	  System.out.println(sum);

	}
}
