

public class Main
{
	public static void main(String[] args) {
	   int [] []a={{1,2,3},
	               {1,2,3},
	               {1,2,3}};
	              
	              
	            
	     
	      int sum=0;
	    int n=a.length;
	    for(int i=0;i<n;i++){
	        
	            
	            
	       
	        for(int j=0;j<n;j++){ 
	            sum=sum+a[i][j];
	        }
	            
	    }
	     System.out.println(sum);
	    }
		
	}
